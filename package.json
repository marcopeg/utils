{
  "name": "@marcopeg/utils",
  "version": "1.14.6",
  "description": "Javascript utils for server and client projects",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/babel-cli/bin/babel.js --watch src --out-dir lib",
    "build:lib": "node_modules/babel-cli/bin/babel.js src-lib --out-dir lib",
    "build:services": "node_modules/babel-cli/bin/babel.js src-services --out-dir services",
    "build": "npm run build:lib && npm run build:services",
    "go": "npm run build && npm publish --access public ."
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-react-jsx",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "author": "Marco Pegoraro",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcopeg/utils.git"
  },
  "bugs": {
    "url": "https://github.com/marcopeg/utils/issues"
  },
  "homepage": "https://github.com/marcopeg/utils#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
